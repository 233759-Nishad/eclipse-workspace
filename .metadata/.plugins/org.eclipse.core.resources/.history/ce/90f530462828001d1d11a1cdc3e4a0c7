package ustdemo;

public class npattern {
	public static void main(String[] args) {
		int i;
		for(i=1;i<=3;i++) 
		{
		if(i==2)
		{
				System.out.println((i)+""+(i-1)+"");
				System.out.println();
		}
		else 
	{
			for(int j=1;j<=3;j++) {
		System.out.println(i+" "+j);
		System.out.println();
		
		}
	}
		

		}
	}
}
