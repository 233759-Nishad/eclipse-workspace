package ustdemo;

public class npattern {
	public static void main(String[] args) {
		int i;
		for(i=1;i<=8;i++) 
		{
		while(i<=3)
		{
				System.out.println(1+""+(i)+"");
		i=i+1;
		}
	while(i==4)
	{
		System.out.println(""+2+""+1);
		i=i+1;
		
		}
	while(i<=8)
	{
			System.out.println(3+""+(i)+"");
	i=i+1;
	
	}
		

		}
	}
}
